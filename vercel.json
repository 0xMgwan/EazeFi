{
  "version": 2,
  "buildCommand": "cd client && npm install --legacy-peer-deps && CI=false npm run build",
  "outputDirectory": "client/build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CI": "false"
  }
}
