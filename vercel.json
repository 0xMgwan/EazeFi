{
  "version": 2,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "client/build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  }
}
